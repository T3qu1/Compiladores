PROGRAM ::= BLOCK
BLOCK ::= FUNC_M 
FUNC_M  ::= FUNC FUNC_M
FUNC_M  ::= ''
FUNC ::= TYPE uma ID parenl PARAM_DEF_M parenr keyl BODY CHAYA keyr
FUNC ::= ''
PARAM_DEF_M ::= TYPE ID PARAM_DEF 
PARAM_DEF_M ::= ''
PARAM_DEF ::= comma TYPE ID PARAM_DEF
PARAM_DEF ::= ''
PARAM_M ::= E PARAM_E
PARAM_M ::= ''
PARAM_E ::= comma E PARAM_E
PARAM_E ::= ''
CHAYA ::= chaya E dotcomma
TYPE ::= int 
TYPE ::= chillu
BODY ::= TYPE STATEMENT BODY
BODY ::= ''
STATEMENT ::= ID assign E dotcomma
STATEMENT ::= DECL
DECL ::= ID dotcomma
DECL ::= FUNC_DECL
STATEMENT ::= ARI_DECL 
STATEMENT ::= PACHA_DECL 
STATEMENT ::= HAYKAQ_DECL 
E ::= T E'
E' ::= OPER T E' 
E' ::= ''
T ::= parenl E parenr
T ::= TERM
ARI_DECL ::= ari parenl COMPARATION parenr keyl E keyr SHUC_DECL
SHUC_DECL ::= shuc keyl E keyr 
SHUC_DECL ::= ''
PACHA_DECL ::= pacha parenl COMPARATION parenr keyl E keyr
HAYKAQ_DECL ::= haykaq parenl F dotcomma COMPARATION dotcomma ID opemasmas parenr keyl E keyr
F ::= int ID assign TERM
COMPARATION ::= TERM OPER_COMP TERM
OPER_COMP ::= kitin 
OPER_COMP ::= chulla
OPER_COMP ::= chana
OPER_COMP ::= kuraq 
OPER_COMP ::= chanakitin
OPER_COMP ::= kuraqkitin
TERM ::= TERM_FUNC
TERM ::= niqin
TERM ::= kundi
TERM_FUNC ::= parenl PARAM_M parenr
TERM_FUNC ::= ''
ID ::= id
ID ::= ''
OPER ::= o 
OPER ::= war
OPER ::= yapa
OPER ::= minus
OPER ::= achka
OPER ::= pat
OPER ::= yallin
